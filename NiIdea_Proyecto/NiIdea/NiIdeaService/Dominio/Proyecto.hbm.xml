<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="NiIdeaService"
                   namespace="NiIdeaService.Dominio"
                   default-lazy="false">
	<class name="Proyecto" table="t_Proyecto">
		<id name="codigo_p" column="codigo_p">
			<generator class="identity" />
		</id>
		<property name="nombre" column="nombre" />
		<property name="fecha_ini" column="fecha_ini" />
		<property name="fecha_fin" column="fecha_fin" />
		<property name="responsable" column="responsable" />
		<property name="observacion" column="observacion" />
		<property name="codigo_c" column="codigo_c" />
		<property name="estado" column="estado" />
    <property name="cierreProyecto" column="estadoCierre" />
    <property name="detalleProyecto" column="descCierre " />
    
	</class>
</hibernate-mapping>

